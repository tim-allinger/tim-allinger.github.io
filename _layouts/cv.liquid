---
layout: default
---
<div class="post">
  <header class="post-header">
    <h1 class="post-title" style="font-size: 3em;">{{ page.title }}</h1>
  </header>

  {% if page.cv_pdf %}
    <div style="margin-top: 1.5rem;">
      <iframe src="{{ page.cv_pdf | prepend: 'assets/pdf/' | relative_url }}#view=FitH"
              width="100%"
              height="900px"
              style="border: none;">
      </iframe>
    </div>
  {% endif %}
</div>
